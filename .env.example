# Stripe API Keys
STRIPE_PUBLIC_KEY=your_stripe_public_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here
WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Render/Mistral AI Server
MISTRAL_API_URL=https://your-mistral-instance.com/api
MISTRAL_API_KEY=your_mistral_api_key_here

# AWS S3 (or Render Static Storage) for Report PDFs
S3_BUCKET_NAME=your_s3_bucket_name_here
S3_ACCESS_KEY=your_s3_access_key_here
S3_SECRET_KEY=your_s3_secret_key_here

# Database (Optional, if you're storing metadata)
DATABASE_URL=postgresql://user:password@host:port/dbname

# Rewardful Affiliate Program
REWARDFUL_API_KEY=your_rewardful_api_key_here

# Email Service (For PDF delivery)
EMAIL_HOST=smtp.your-email-provider.com
EMAIL_PORT=587
EMAIL_USER=your_email_username_here
EMAIL_PASSWORD=your_email_password_here

# Deployment Settings
FRONTEND_URL=https://yourfrontendurl.com
BACKEND_URL=https://yourbackendurl.com
